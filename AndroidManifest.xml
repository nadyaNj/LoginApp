<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="am.home.LoginApp"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="15"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name="LoginActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="am.home.LoginApp.UserPageActivity"
                android:label="@string/user_page"
                android:parentActivityName="am.home.LoginApp.LoginActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="am.home.LoginApp.LoginActivity" />
        </activity>
    </application>
</manifest>
